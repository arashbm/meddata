<%= simple_form_for(@article) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pubmed_id %>
    <%= f.input :title %>
    <%= f.input :abstract %>
    <%= f.input :raw_pubmed_xml %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
